{
  "$schema": "https://json.schemastore.org/angular.json",
  "version": 1,
  "projects": {
    "booking-frontend": {
      "projectType": "application",
      "root": "",
      "sourceRoot": "src",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/booking-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ]
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "booking-frontend:build",
            "port": 4200,
            "host": "0.0.0.0"
          }
        }
      }
    }
  },
  "defaultProject": "booking-frontend"
}